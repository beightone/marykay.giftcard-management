{
  "name": "giftcard-manager",
  "vendor": "vtex",
  "version": "0.0.12",
  "title": "GiftCard Admin",
  "description": "",
  "mustUpdateAt": "2022-08-28",
  "scripts": {
    "postreleasy": "vtex publish"
  },
  "dependencies": {
    "vtex.styleguide": "9.x"
  },
  "policies": [
    {
      "name": "colossus-fire-event"
    },
    {
      "name": "colossus-write-logs"
    },
    {
      "name": "view_giftcards"
    },
    {
      "name": "edit_giftcards"
    },
    {
      "name": "ADMIN_CRM"
    },
    {
      "name": "POWER_USER_CRM"
    },
    {
      "name": "ADMIN_DS"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com",
        "path": "/api/giftcards/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.vtex.com",
        "path": "/dataentities/*"
      }
    },
    {
      "name": "POWER_USER_DS"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.myvtex.com",
        "path": "/api/giftcards/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "marykay.myvtex.com",
        "path": "/api/giftcards/"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "marykay.myvtex.com",
        "path": "/api/giftcards/"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.myvtex.com",
        "path": "/api/dataentities/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "marykay.myvtex.com",
        "path": "/api/dataentities/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "http-intake.logs.datadoghq.com/",
        "path": "*"
      }
    }
  ],
  "builders": {
    "node": "7.x",
    "react": "3.x",
    "admin": "0.x",
    "graphql": "2.x",
    "messages": "1.x",
    "docs": "0.x"
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
